{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "eslint-plugin-import/no-extraneous-dependencies":"off",
    "no-underscore-dangle":"off",
    "camelcase": "off"
  }
}
